@article{liu_drivers_2011,
	title = {Driver's visual cognition behaviors of traffic signs based on eye movement parameters},
	volume = {11},
	issn = {10096744},
	url = {http://dx.doi.org/10.1016/S1570-6672(10)60129-8},
	doi = {10.1016/s1570-6672(10)60129-8},
	abstract = {A dynamic visual cognition experiment was carried out in driving simulation barn with eye tracker to get drivers' eye movement parameters when they were reading traffic signs with various information quantities at different speeds. The eye movement parameters include fixation number and duration. Driver' s visual area was divided into three areas using dynamic cluster method. Further analysis was conducted on the eye movement parameters of the three areas. Findings were discussed in this paper; the fixation distribution has a high correlation with driving speed and information quantity. As the driving speed and information quantity increased, the fixation number and duration in traffic sign area have an increasing trend and drivers have less visual cognition to front lane. When the guide signs have more than five amount of information, drivers' fixation number and duration have an abrupt change and the effectiveness of visual cognition decreased significantly. It is recommended that the highway traffic guide sign information volume should not exceed 5. Under the same condition, the driver needs more fixation when recognizing non-guide signs and the visual cognition process is more complicated. ©2011 by Science Press.},
