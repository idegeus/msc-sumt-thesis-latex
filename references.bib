@article{weingart_2_2018,
	title = {2. {Interdisciplinarity}: {The} {Paradoxical} {Discourse}},
	doi = {10.3138/9781442678729-004},
	abstract = {Alle Beschwörungen von Interdisziplinarität als neue Wissenschaftsform hatten wenig Einfluss auf die tatsächliche wissenschaftliche Praxis trotzdem geht der Diskurs über Interdisziplinarität unvermindert weiter warum? Die These ist, dass auf einer sozialpsychologischen Ebene die Werte der Universalität und des organisierten Skeptizismus Offenheit gegenüber allem relevanten Wissen suggerieren, das wird übersetzt in Interdisziplinarität auf einem programmatischen Level, gleichbedeutend mit Innovation das ist eine Ambivalenz von Wissenschaft, denn normalerweise verläuft die Produktion von Wissen nach einer anderen Strategie: man schaut sich nach einer Nische um so werden immer neue und feinere Strukturen etabliert Aber was bringt die Wissenschaftspolitik dazu, auf Transdisziplinarität zu setzen? Deren Motive müssen unterschiedlich sein Der Interdisziplinaritäts-Diskurs ist ein Diskurs über Innovation der Wissensproduktion Die Hoffnung für Innovation liegt in der Zukunft, aber die Zukunft ist unbekannt und unstrukturiert das könnte einiges der Hoffnungen erklären, die auf Interdisziplinarität projiziert werden Deklarationen von Wissenschaftern über die wünschenswerte transdisziplinäre Forschung können nicht für bare Münze genommen werden, auch kann nicht erwartet werden, dass normative Appelle das Verhalten der Wissenschafter ändern wird. Man kann das hingegen als Opportunismus in der Suche nach Neuem, kontrolliert von Standards und Kompetenzen, betrachten Interdisziplinarität als Formen der Forschungsorganisation: In den meisten Forschungsprogrammen, die Interdisziplinarität fordern und fördern, wird kaum systematisch kontrolliert, ob das tatsächlich passiert z.B. erzählte ein Insider auf einer Konferenz dass bei einem solchen Progamm nur 20\% der Projekte tatsächlich interdisziplinär waren, das ist weniger als im Standardforschungsprogramm wo keine Interdisziplinarität gefordert ist das kann so interpretiert werden, dass Interdisziplinarität dem Opportunismus des Antragschreibens dient oder dass es einfach nur problemspezifische und problemgesteuerte Wissenskomplexe meint meist bleibt es ein rein formales Label, das vor allem ein Organisationsprinzip ist aus der Sicht der Forschungsprogramme ist Interdisziplinarität v.a. von politischen Zielen und Legitimationserfordernissen motiviert unter Umständen von limitierten Ressourcen und Legitimationsdruck werden Wissenschafter Problemdefinitionen und Labels erfinden, die zu den Erwartungen der Öffentlichkeit und der Politik passen trotz allem muss eine gewisse Struktur vorhanden sein und auch interdisziplinäre Forschung hat ihre blinden Flecken der Unterschied zwischen disziplinärer und interdisziplinärer Forschung liegt nicht in einer größeren Nähe zur Realität sondern in anderen sozialen Umständen der Forschung auch interdisziplinäre Forschung hält sich an die grundsätzlichen Regeln der Wissensproduktion der Schlüssel liegt darin, dass man Interdisziplinarität mit Innovation assoziiert: man kann Innovation nicht willentlich erzeugen, man kann nur die Umstände der Wissenserzeugung so beeinflussen, dass ein kreatives Umfeld entsteht so gesehen ist Interdisziplinarität ein anderer Weg zu sagen, dass man wissenschaftliche Innovation durch freies Fließen von Opportunismus in der Wissensproduktion organisieren möchte um Innovation zuzulassen, müssen organisationale Strukturen flexibel und diversifiziert gehalten werden insofern sind Interdisziplinarität und Spezialisierung parallele Entwicklungen},
	journal = {Practising Interdisciplinarity},
	author = {Weingart, Peter},
	year = {2018},
	pages = {25--42},
}

